                                =
\"$texto\"&lt;br&gt;&lt;br&gt;";<BR>?&gt;</P>
                                <P class=3Dcodigo_flash>&lt;form =
method=3Dpost=20
                                action=3D"&lt;? echo $PATH_INFO;=20
                                ?&gt;"&gt;<BR>&lt;input type=3D"text" =
name=3D"texto"=20
                                value=3D"" =
size=3D10&gt;<BR>&lt;br&gt;<BR>&lt;input=20

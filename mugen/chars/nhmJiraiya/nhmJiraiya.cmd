; 下の記述↓は絶対に消さないでください。
[Statedef -1]

;-------------------------------------------------------------------
;heavy punch
[state -1, Heavy Punch]
type = ChangeState
value = 230
triggerall = stateno = 100
triggerall = command = "a"
triggerall = command != "holddown"
trigger1 = statetype = S
trigger1 = ctrl

;-------------------------------------------------------------------
;heavy punch
[state -1, Grab]
type = ChangeState
value = 800
triggerall = command = "a+b"
triggerall = command != "holddown"
trigger1 = statetype = S
trigger1 = ctrl


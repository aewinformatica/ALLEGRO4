; Definition file for kfm
; Contains all the filenames needed for the character

; Player information
[Info]
name = "Shikamaru"        ;Name of character
displayname = "Shikamaru" ;Name of character to display
versiondate = 11,22,2000    ;Version date of character
mugenversion = 11,22,2000   ;Version of M.U.G.E.N character works on
author = "jordz187"         ;Character author name
pal.defaults = 6,3,4,2      ;Default palettes in order of preference

; Files for the player
[Files]
cmd     = shikamaru.cmd   ;Command set
cns     = shikamaru.cns   ;Constants
st      = shikamaru.cns   ;States
stcommon = common1.cns ;Common states
sprite  = shikamaru.sff   ;Sprite
anim    = shikamaru.air   ;Animation
sound   = shikamaru.snd   ;Sound
pal1    = shikamaru.act  ;Palettes (can have up to 12)

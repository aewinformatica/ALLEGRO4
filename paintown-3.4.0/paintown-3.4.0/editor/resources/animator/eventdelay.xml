<panel layout="GridBagLayout">
	<label text="Set Delay">
		<gridbagconstraints
			gridx="0"
			gridy="0"
			weightx="0"
			weighty="1"
			gridheight="1"
			anchor="GridBagConstraints.NORTHWEST"
			fill="GridBagConstraints.BOTH"
			/>
	</label>
	<spinner id="delay">
		<gridbagconstraints
			gridx="1"
			gridy="0"
			weightx="1"
			weighty="1"
			gridheight="1"
			anchor="GridBagConstraints.NORTHWEST"
			fill="GridBagConstraints.BOTH"
			/>

	</spinner>
</panel>
